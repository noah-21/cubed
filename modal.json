{
    "dimensions": [
        {"name": "name", "label": "Name"},
        {"name": "category", "label": "Category"},
        {"name": "keywords", "label": "Keywords"},
        {"name": "author", "label": "Author"},
	{"name": "no_of_images", "label": "Number of Images"},
	{"name": "no_of_videos", "label": "Number of Videos"},
	{"name": "head_len", "label": "Heading Length"},
	{"name": "no_of_abusive_words", "label": "Number of Abusive Words"}
	{
            "name": "pub_date",
            "label": "Publish Date",
            "info": {
                "cv-datefilter": true,
		"cv-datefilter-hierarchy": "daily"
            },
            "levels": [
                {
                    "name":"year",
                    "label":"Year",
                    "info": { "cv-datefilter-field": "year" }
                },
                {
                    "name":"month",
                    "label":"Month"
                },
		{
		    "name":"day",
		    "label":"Day"
		},
		{
		    "name": "hour",
		    "label": "Hour"
		},
		{
		    "name": "mins",
		    "label": "Minutes"
		}
            ],
            "hierarchies": [
                {
                    "name": "daily",
                    "label": "Daily",
                    "levels": [ "year", "month", "day"]
                },

		{
		    "name": "hourly",
		    "label": "Hourly",
                    "levels": [ "hour", "mins"]
		}
            ]
        }
    ],
    "cubes": [
        {
            "name": "FB_POSTS_DATA",
            "dimensions": ["name", "category", "author", "pub_date", "no_of_images", "no_of_videos", "head_len", "no_of_abusive_words"],
            "measures": [
                {"name": "like"},
                {"name": "share"},
                {"name": "comment"},
                {"name": "ctr"}
            ],
            "aggregates": [
                {
                    "name": "total_likes",
                    "function": "sum",
                    "measure": "like"
                },

                {
                    "name": "total_shares",
                    "function": "sum",
                    "measure": "share"
                },

                {
                    "name": "min_likes",
                    "function": "min",
                    "measure": "like"
                },

                {
                    "name": "min_shares",
                    "function": "min",
                    "measure": "share"
                },

                {
                    "name": "max_likes",
                    "function": "max",
                    "measure": "like"
                },

                {
                    "name": "max_shares",
                    "function": "max",
                    "measure": "share"
                },

                {
                    "name": "avg_likes",
                    "function": "avg",
                    "measure": "like"
                },

                {
                    "name": "avg_shares",
                    "function": "avg",
                    "measure": "share"
                },

                {
                    "name": "total_comments",
                    "function": "sum",
                    "measure": "comment"
                },

                {
                    "name": "total_ctr",
                    "function": "sum",
                    "measure": "ctr"
                },

                {
                    "name": "min_comments",
                    "function": "min",
                    "measure": "comment"
                },

                {
                    "name": "min_ctr",
                    "function": "min",
                    "measure": "ctr"
                },

                {
                    "name": "max_comments",
                    "function": "max",
                    "measure": "comment"
                },

                {
                    "name": "max_ctr",
                    "function": "max",
                    "measure": "ctr"
                },

                {
                    "name": "avg_comments",
                    "function": "avg",
                    "measure": "comment"
                },

                {
                    "name": "avg_ctr",
                    "function": "avg",
                    "measure": "ctr"
                },

                {
                    "name": "record_count",
                    "function": "count"
                }
            ],
	    "mappings":{
		"pub_date.year": "year",
		"pub_date.month": "month",
		"pub_date.day": "day",
		"pub_date.hour": "hour",
		"pub_date.mins": "mins"
	    } 
        },
        {
            "name": "KEYWORDS_DATA",
            "dimensions": ["keywords", "pub_date"],
            "measures": [
                    {"name": "like"},
                {"name": "share"},
                {"name": "comment"},
                {"name": "ctr"}
            ],
            "aggregates": [
                {
                    "name": "total_likes",
                    "function": "sum",
                    "measure": "like"
                },

                {
                    "name": "total_shares",
                    "function": "sum",
                    "measure": "share"
                },

                {
                    "name": "min_likes",
                    "function": "min",
                    "measure": "like"
                },

                {
                    "name": "min_shares",
                    "function": "min",
                    "measure": "share"
                },

                {
                    "name": "max_likes",
                    "function": "max",
                    "measure": "like"
                },

                {
                    "name": "max_shares",
