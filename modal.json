                    "measure": "comment"
                },

                {
                    "name": "max_ctr",
                    "function": "max",
                    "measure": "ctr"
                },

                {
                    "name": "avg_comments",
                    "function": "avg",
                    "measure": "comment"
                },

                {
                    "name": "avg_ctr",
                    "function": "avg",
                    "measure": "ctr"
                },

                {
                    "name": "record_count",
                    "function": "count"
                }

            ],

	    "mappings":{
                "pub_date.year": "year",
